{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@flowaccount/aws-amplify-angular",
  "version": "4.2.1",
  "description": "AWS Amplify Angular Components",
  "main": "dist/bundles/flowaccount-aws-amplify-angular.umd.min.js",
  "module": "dist/fesm5/flowaccount-aws-amplify-angular.js",
  "es2015": "dist/fesm2015/flowaccount-aws-amplify-angular.js",
  "esm5": "dist/esm5/flowaccount-aws-amplify-angular.js",
  "esm2015": "dist/esm2015/flowaccount-aws-amplify-angular.js",
  "fesm5": "dist/fesm5/flowaccount-aws-amplify-angular.js",
  "fesm2015": "dist/fesm2015/flowaccount-aws-amplify-angular.js",
  "typings": "dist/flowaccount-aws-amplify-angular.d.ts",
  "sideEffects": false,
  "scripts": {
    "cleantemp": "rimraf coverage dist tmp docs",
    "test": "jest -w 1 --maxWorkers 2",
    "transpile": "ngcc",
    "package": "rollup -c rollup.config.js",
    "minify": "uglifyjs dist/bundles/aws-amplify-angular.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/aws-amplify-angular.umd.min.js",
    "build": "ng-packagr -p package.json",
    "build:esm:watch": "ngcc --watch",
    "build:cjs:watch": "echo \"CommonJS modules are not exported in angular. Use target build:esm:watch\"",
    "format": "echo \"Not implemented\""
  },
  "ngPackage": {
    "assets": [
      "assets/js/aws-lex-audio.js"
    ],
    "lib": {
      "entryFile": "index.ts",
      "umdId": "@flowaccount/aws-amplify-angular",
      "umdModuleIds": {
        "@aws-amplify/core": "@aws-amplify/core",
        "lodash": "lodash",
        "@aws-amplify/ui": "@aws-amplify/ui",
        "util": "util"
      }
    },
    "whitelistedNonPeerDependencies": [
      "@aws-amplify/ui",
      "rxjs-compat"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic)"
    ]
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "devDependencies": {
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/forms": "^9.0.7",
    "@angular/platform-browser": "^9.0.7",
    "@angular/localize": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.4.6",
    "@types/paho-mqtt": "^1.0.3",
    "@types/zen-observable": "^0.5.3",
    "aws-amplify": "^2.2.7",
    "css-loader": "^0.28.1",
    "jest-preset-angular": "^7.1.1",
    "json-loader": "^0.5.4",
    "ng-mocks": "7.x.x",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^6.5.4",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "uglify-js": "^3.4.7",
    "zone": "^0.3.4",
    "zone.js": "^0.10.3",
    "ng-packagr": "^9.0.3",
    "@angular/cli": "^9.0.7",
    "tslib": "^1.10.0",
    "typescript": "3.7.5",
    "rollup-plugin-node-builtins": "^2.1.2"
  },
  "dependencies": {
    "@aws-amplify/ui": "^1.1.6",
    "rxjs-compat": "^6.5.2"
  },
  "peerDependencies": {
    "aws-amplify": "^2.0.0"
  }
}